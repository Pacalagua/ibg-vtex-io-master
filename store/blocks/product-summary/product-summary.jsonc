      "product-price-savings#summary"
    ]
  },
  "product-installments#summary": {
    "props": {
      "blockClass": "summary"
    }
  }, */
  "product-selling-price#summary": {
    "props": {
      "blockClass": "summary",
      "message": "{sellingPriceValue} \n Aliados Cr√©dito"
    }
  },
  "product-price-savings#summary": {
    "props": {
      "markers": [
        "discount"
      ],
      "blockClass": "summary"
    }
  },
  "flex-layout.col#product-price": {
    "props": {
      "blockClass": "product-price"
    },
    "children": [
      "flex-layout.col#tercer-precio",
      "product-selling-price#summary",
      "product-list-price#summary"
    ]
  },
  "flex-layout.col#product-price-pdp": {
    "props": {
      "blockClass": "product-price-pdp"
    },
    "children": [
      "flex-layout.col#tercer-precio-pdp",
      "product-selling-price#summary",
      "product-list-price#summary"
    ]
  },
  "flex-layout.col#tercer-precio": {
    "props": {
      "blockClass": "tercer-precio-vitrinas"
    },
    "children": [
      "TercerPrecio"
    ]
  },
  "flex-layout.col#category-discount-plp": {
    "props": {
      "blockClass": "CategoryDiscount-plp"
    },
    // "blockClass": "pdpFlag",
    "children": [
      /* "DiscountCategory" */
    ]
  },
  "flex-layout.col#tercer-precio-pdp": {
    "props": {
      "blockClass": "tercer-precio-vitrinas-pdp"
    },
    "children": [
      "TercerPrecio"
    ]
  }
}
